{
	"name": "Roblox Studio Light",
	"colors": {
		"editor.background": "#ffffff",
		"editor.foreground": "#000000",
		"editorWhitespace.foreground": "#bcbec8" // Whitespace Color
	},
	"tokenColors": [
		{
			"name": "'local' Color",
			"scope": [
				"keyword",
				"storage"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#001B7A"
			}
		},
		{
			"name": "Type Color",
			"scope": [
				"storage.type",
				"support.type"
			],
			"settings": {
				"foreground": "#0C9B5A"
			}
		},
		{
			"name": "Text Color",
			"scope": [
				"variable",
				"support.variable"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Function Name Color",
			"scope": [
				"entity.name.function",
				"support.function"
			],
			"settings": {
				"foreground": "#002DD6"
			}
		},
		{
			"name": "Local Property Color",
			"scope": "variable.other.property.luau",
			"settings": {
				"foreground": "#6A6f81"
			}
		},
		{
			"name": "Type Color",
			"scope": [
				"entity.name.type"
			],
			"settings": {
				"foreground": "#0C9B5A"
			}
		},
		{
			"name": "Operator Color",
			"scope": [
				"keyword.operator",
				"source.luau",
				"punctuation.arguments"
			],
			"settings": {
				"foreground": "#976C00"
			}
		},
		{
			"name": "Number Color",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#0C9B5A"
			}
		},
		{
			"name": "String Color",
			"scope": "string",
			"settings": {
				"foreground": "#6E0785"
			}
		},
		{
			"name": "Comment Color",
			"scope": "comment",
			"settings": {
				"foreground": "#015F35"
			}
		},
		{
			"name": "Built-in Function Color",
			"scope": [
				"constant.language",
				"support.constant",
				"variable.language"
			],
			"settings": {
				"foreground": "#830F07"
			}
		},
	]
}